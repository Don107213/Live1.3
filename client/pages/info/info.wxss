page{
  background: #f6f6f6;
}
.container {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
}

.userinfo {
  display: flex;
  flex-direction: column;
  margin-top: 100rpx;
  align-items: center;
}

.userinfo-avatar {
  width: 130rpx;
  height: 130rpx;
}

.userinfo-nickname {
  margin-top: 20rpx;
  font-size: 32rpx;
  color: black;
}

.userinfo-nickname::after {
  border: none;
}
